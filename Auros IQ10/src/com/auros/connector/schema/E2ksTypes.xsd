<?xml version="1.0" encoding="UTF-8" standalone="yes"?>


<!-- avoid attributes named "id". They cause problems in gsoap at least-->
<!-- avoid attributes named "id". They cause problems in gsoap at least-->

<xs:schema version="1.0"  targetNamespace="http://e2ks.emergent.com/types" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xmime="http://www.w3.org/2005/05/xmlmime"  xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
            jxb:version="1.0"
xmlns="http://e2ks.emergent.com/types"
elementFormDefault="qualified"
>

  <!-- Generic Version Information. Runtime Client side branching decisions can be based on this.  -->
  <xs:complexType name="E2KSInstallationInfo">
    <xs:sequence>
      <xs:element name="version" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="customer" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="serverInstance" type="xs:string" minOccurs="1"  maxOccurs="1" />
    </xs:sequence>
  </xs:complexType >

  <xs:complexType name="RuleColumn">
    <xs:sequence>
      <xs:element name="columnName" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="columnType" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="ruleParameter" type="Parameter" minOccurs="1"  maxOccurs="1" />
      <!-- list of parameters needed to evaluate this column. Aggregate of parameters for each value  -->
      <xs:element name="neededParameters" type="Parameter" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="logicalStatement" type="xs:string" minOccurs="1"  maxOccurs="1" />

      <xs:element name="columnPosition" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="functionOf" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="function" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="kbePreCondition" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="kbeURL" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="kbeOtherInfo" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="tolerance1" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="tolerance1Type" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="tolerance2" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="tolerance2Type" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="grouping" type="xs:string" minOccurs="0"  maxOccurs="1" />
      <xs:element name="groupingName" type="xs:string" minOccurs="0"  maxOccurs="1" />

      <xs:element name="linkType" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="linkedKPac" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="prettyName" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="idx" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="dependentColumn" type="xs:string" minOccurs="1"  maxOccurs="1" />
	  
      <xs:element name="color" type="xs:string" minOccurs="1"  maxOccurs="1">
		<xs:annotation>
		  <xs:appinfo xml:space="default">
			<jxb:property>
					 <jxb:javadoc>The segmented color score of the column. This is different from columnType being "color" which means that cell values of the column are colors.</jxb:javadoc>
			</jxb:property> 
		  </xs:appinfo>
		</xs:annotation>	  
	  </xs:element>
      <xs:element name="values" type="xs:string" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="ruleColumnProperties" type="E2KSAttribute" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="prettyNameIdx" type="xs:string" minOccurs="0"  maxOccurs="1" />
      <xs:element name="comment" type="xs:string" minOccurs="0"  maxOccurs="1" />
      <xs:element name="isDefinition" type="xs:string" minOccurs="0"  maxOccurs="1" />



    </xs:sequence>
  </xs:complexType >


  <xs:complexType name="AurosIdentifier">
    <xs:sequence>

      <xs:element name="identifiers" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="type" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>

  </xs:complexType>

  <xs:complexType name="AurosAttachment">
    <xs:annotation>
      <xs:documentation xml:space="default">
        AurosAttachment represents files associated with various Auros Objects such as K-Pacs etc.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>

      <xs:element name="attachmentIdentifier" type="AurosIdentifier" minOccurs="1" maxOccurs="1" />
      <xs:element name="fileTypeID" type="xs:int"  />
      <xs:element name="fileName" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="longDescription" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="documentDescription" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="supportDocText" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="fileOrder" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="content" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="consecutiveURLValue" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="proposeMode" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="outOfDate" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="uploadedBy" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="uploadedOn" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="mediaType" type="xs:string"  />
      <xs:element name="width" type="xs:int"  />
      <xs:element name="height" type="xs:int"  />

    </xs:sequence>
  </xs:complexType >

  
  <xs:complexType name="KPac">

    <xs:annotation>
      <xs:documentation xml:space="default">
        KPac is the fundamental representation of Knowledge items in E2KS. A KPac contains meta data, documentation, etc., of rules or requirements.
        The rules are structured in a tabular form and allow capture and use of Know-how in multiple contexts or applications such as CAD systems, PLM systems etc.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="kpacHeader" type="KPacHeader" minOccurs="1" maxOccurs="1" />


      <xs:element name="justification" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="measurementRef" type="xs:string" minOccurs="1"  maxOccurs="1" />

      <xs:element name="additionalInfo" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="kpacType" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="kpacGroupType" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="versionId" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="contact" type="xs:string" minOccurs="1"  maxOccurs="1" />

      <xs:element name="riskLevel" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="bornOnDate" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="sunSetDate" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="lastUpdateDate" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="publishedDate" type="xs:string" minOccurs="1"  maxOccurs="1" />
	  
      <xs:element name="status" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="author" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="origination" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="compositeVitalityScore" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="nonConformanceComponent" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="usageComponent" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="sunsetComponent" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="publicAccessPolicy" type="xs:string" minOccurs="1"  maxOccurs="1" />

      <xs:element name="ruleColumns" type="RuleColumn" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="productCodes" type="xs:string" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="requiredOfs" type="xs:string" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="supportDocuments" type="AurosAttachment" minOccurs="0"  maxOccurs="unbounded" />

      <xs:element name="kpacAttributes" type="E2KSAttribute" minOccurs="0"  maxOccurs="unbounded" />


      <xs:element name="kpacAttachments" type="AurosAttachment" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="vtAttachments" type="AurosAttachment" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="validationMessages" type="Message" minOccurs="0"  maxOccurs="unbounded" />
	  <xs:element name="kpacChapters" type="Chapter" minOccurs="0"  maxOccurs="unbounded" />
	  <xs:element name="methodDetails" type="Method" minOccurs="0"  maxOccurs="1" />
    </xs:sequence>
  </xs:complexType >


  <xs:complexType name="E2KSAttributeList">
    <xs:sequence>
      <xs:element name="containerID" type="AurosIdentifier" minOccurs="1" maxOccurs="1" />
      <xs:element name="exists" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="attributeValue" type="E2KSAttribute" minOccurs="0"  maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType >


  <xs:complexType name="E2KSAttribute">
    <xs:sequence>
      <xs:element name="attributeName" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="attributeValue" type="xs:string" minOccurs="1"  maxOccurs="unbounded" />
      <xs:element name="attributeType" type="xs:string" minOccurs="1" maxOccurs="1" />
	  <xs:element name="attributeDataType" type="xs:string" minOccurs="1" maxOccurs="1" />
	  <xs:element name="attributeOrder" type="xs:int" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType >
  


  <!--  To be removed InputOptions to be used instead -->
  <xs:complexType name="OutputOptions">
    <xs:sequence>
      <xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="value" type="xs:string" minOccurs="1"  maxOccurs="1" />
    </xs:sequence>
  </xs:complexType >

  <!-- Things like how a webcall is processed, when several loosely tied possibilities exist. Eg. "UseTargetValues=True" 
 This also provides a way to extend call  behavior without changing the schema,wsdl for minor bug fixes, enhancements. -->
  <xs:complexType name="InputOptions">
    <xs:sequence>
      <xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="value" type="xs:string" minOccurs="1"  maxOccurs="1" />
    </xs:sequence>
  </xs:complexType >



  <!-- holds identifying and content descriptive information -->
  <xs:complexType name="CoPHeader">
    <xs:sequence>
      <xs:element name="name" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="description" type="xs:string" minOccurs="1"  maxOccurs="1" />
	  <xs:element name="categoryPath" type="xs:string" minOccurs="1"  maxOccurs="1" />

    </xs:sequence>
    <xs:attribute name="copId" type="xs:string"/>
  </xs:complexType >



  <xs:complexType name="CoP">
    <xs:sequence>

      <xs:element name="copHeader" type="CoPHeader" minOccurs="1"  maxOccurs="1" />

      <xs:element name="sunsetIncrement" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="openAuthorship" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="publicAccessPolicy" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="assessmentTrackingYN" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="tclAuthorshipPolicyId" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="assessmentTitle" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="projectDisplayName" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="mileStoneDisplayName" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="subProjectDisplayName" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="mapExtRegToSubProject" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="mapRegId" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="parameterCheckName" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="mapRegIdToMileStone" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="assessmentAttachments" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="assessmentActuals" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="assessmentApprovals" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="assessmentApprovalType" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="projectProjSpaceMapping" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="subProjectProjSpaceMapping" type="xs:string" minOccurs="1"  maxOccurs="1" />

      <xs:element name="startupPage" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="startupKpac" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="confGlobalPalletId" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="assessmentTargets" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="assessmentDiscussion" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="assessmentParamView" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="assessmentBasicView" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="assessmentCascadeView" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="assessmentDefaultView" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="raiseIssue" type="xs:string" minOccurs="1"  maxOccurs="1" />

      <!-- flexible association of properties to CoP -->
      <xs:element name="copAttributes" type="E2KSAttribute" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="kpacAttributes" type="E2KSAttribute" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="setAttributes" type="E2KSAttribute" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="aceeAttributes" type="E2KSAttribute" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="acseAttributes" type="E2KSAttribute" minOccurs="0"  maxOccurs="unbounded" />
	  <xs:element name="contentKPacHeaders" type="KPacHeader" minOccurs="0"  maxOccurs="unbounded" />
	  <xs:element name="milestones" type="Milestone" minOccurs="0"  maxOccurs="unbounded" />
	  <xs:element name="subProjects" type="xs:string" minOccurs="0"  maxOccurs="unbounded" />


    </xs:sequence>
    <!-- <xs:attribute name="copId" type="xs:string"/> -->
  </xs:complexType >
  
  <xs:complexType name="Milestone">
    <xs:sequence>
		<xs:element name="milestoneVersion" type="xs:string" minOccurs="1"  maxOccurs="1" />
		<xs:element name="milestoneScope" type="xs:string" minOccurs="1"  maxOccurs="1" />
		<xs:element name="milestoneNames" type="xs:string" minOccurs="0"  maxOccurs="unbounded" />
	</xs:sequence>
  </xs:complexType >



  <!-- Dimension tells what fundamental quantity is measured. Eg. Mass, Length, Time -->

  <!-- The documentation in documentation element goes to the generated code in Java, etc. -->
  <xs:complexType name="Dimension">
    <xs:annotation>
      <xs:documentation xml:space="default">
        Dimension tells what fundamental quantity is measured. Eg. Mass, Length, Time
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="descriptiveName" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="exponent" type="xs:double" minOccurs="1"  maxOccurs="1" />
    </xs:sequence>
    <xs:attribute name="name" type="xs:string"/>
  </xs:complexType >

  <!-- CompoundDimension tells what quantity is measured. These are expressed as compounded dimensions, eg. Speed - Length/Time -->
  <xs:complexType name="CompoundDimension">
    <xs:sequence>
      <xs:element name="dimensions" type="Dimension" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="name" type="xs:string"  />
    </xs:sequence>
	
    <xs:attribute name="categoryID" type="xs:int"/>
 </xs:complexType >

  <!-- Unit tells how a quantity is measured w.r.t a standard. For a Dimension there may be multiple standards. Standards may be prototypes or based on physical constants. 
This unit is used to represent a "Compound Unit" also such as mtr*mtr. All Units have a dimension associated, but rather than have this association as an explicit membership of Unit, 
the CompoundDimension is always computed? -->
  <xs:complexType name="Unit">
    <xs:sequence>
      <xs:element name="descriptiveName" type="xs:string" minOccurs="1"  maxOccurs="1" />
	  <xs:element name="compoundDimension" type="CompoundDimension" />
      <xs:element name="validationMessage" type="xs:string" minOccurs="1"  maxOccurs="1" />
	  
    </xs:sequence>
    <xs:attribute name="name" type="xs:string"/>
    <xs:attribute name="categoryID" type="xs:int"/>
    <xs:attribute name="scaleFactor" type="xs:double"/>
    <xs:attribute name="scaleFactorComputed" type="xs:boolean"/>

 	
  </xs:complexType >


  <!-- Represents a Parameter or variable in a system of rules/expressions/equations stored in Value Columns or Rule Columns of K-PACS-->
  <xs:complexType name="Parameter">
    <xs:sequence>
      <xs:element name="name" type="xs:string" minOccurs="1"  maxOccurs="1" />

      <xs:element name="dimension" type="CompoundDimension" minOccurs="0"  maxOccurs="1" />
      <xs:element name="unit" type="Unit" minOccurs="0"  maxOccurs="1" />
      
      <!-- a list of reference values for reference types -->
      <xs:element name="referenceValues" type="xs:string" minOccurs="0"  maxOccurs="unbounded" />

    </xs:sequence>
    <xs:attribute name="dataType" type="xs:string"/>

    <!-- Can hold values such as Nominal, Default, Minimum, Derived etc., 
	 duplication of field columnType in RuleColumn -->
    <xs:attribute name="e2ksUsageType" type="xs:string"/>

    <!-- value to assign when e2ksUsageType="Default" -->
    <xs:attribute name="defaultValue" type="xs:string"/>

  </xs:complexType >

  <!-- Represents Uses of a Parameter in Value Columns/Expressions/Rules as identified in the members -->
  <xs:complexType name="ParameterUse">
    <xs:sequence>
      <xs:element name="paramName" type="xs:string" minOccurs="1"  maxOccurs="1" />

      <!-- equal number of the below three should occur -->
      <!-- every triplet identifies a specific column -->
      <xs:element name="copID" type="xs:string" minOccurs="1"  maxOccurs="unbounded" />
      <xs:element name="kpacID" type="xs:string" minOccurs="1"  maxOccurs="unbounded" />
      <xs:element name="ruleColumnID" type="xs:string" minOccurs="1"  maxOccurs="unbounded" />


      <xs:element name="dimension" type="CompoundDimension" minOccurs="1"  maxOccurs="1" />
    </xs:sequence>

  </xs:complexType >

  <!-- Represents a Use of a Parameter in a specific Value Column/Expression/Rule as identified in the members -->
  <xs:complexType name="ParameterUseNode">
    <xs:sequence>
      <xs:element name="thisParameterUse" type="ParameterUse" minOccurs="1"  maxOccurs="1" />
      <xs:element name="nodesUsedByThis" type="xs:int" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="nodesUsingThis" type="xs:int" minOccurs="0"  maxOccurs="unbounded" />
    </xs:sequence>
    <xs:attribute name="nodeID" type="xs:int"/>

  </xs:complexType >


  <!-- Represents a match of the Parameter in E2ks to a Parameter instance in an external system, typicaly a CAD system. 
Instances have values associated. While ParameterUse relate to rules, Instances relate to facts. There can be multiple instances that
can be applied to a use. E2ks processes the ParameterUses and ParameterInstances and provides validation results. 
paramName and instanceName may be auto matched by patterns or non-automatic, explicit means that have no resemblence between them.

-->
  <xs:complexType name="ParameterInstance">
    <xs:sequence>
      <xs:element name="paramName" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="instanceName" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <!-- The value member is now a String that may hold any type such as bool, int, etc. A better way should be to hold a variant type "ParamValue". todo and explore -->
      <xs:element name="value" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="units" type="Unit" minOccurs="0"  maxOccurs="1" />
      <xs:element name="lastModifiedDate" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="modifiedBy" type="xs:string" minOccurs="0"  maxOccurs="1" />

      <!-- when a parameter use refers a rule that has different parameters and there are instances of these parameters, the need to group the instances arises.
	  This assuming no auto grouping options are used. groupingId is supplied by the client caller and is repsonsible for proper and meaningful grouping. 
	  The groupingId identifies the set of instances to be used in a rule evaluation context. When the larger context is an assessment, the group id is persisted along with reported values. 
	  In other cases the grouping is lost after the evaluation call in the server  -->
      <xs:element name="groupingIDs" type="xs:string" minOccurs="0"  maxOccurs="unbounded" />
      <xs:element name="instanceSourceID" type="xs:string" minOccurs="0"  maxOccurs="1" />

    </xs:sequence>

  </xs:complexType >

  <!-- Identifies a K-PAC -->

  <xs:complexType name="KPacHeader">
    <xs:annotation>
      <xs:documentation xml:space="default">
        KPacHeader idetifies and provides a top level description of a KPac
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:annotation>
        <xs:documentation>
          KPac id, which along with copID uniquely identifies a KPac
        </xs:documentation>
      </xs:annotation>
      <xs:element name="kpacID" type="xs:string" minOccurs="1"  maxOccurs="1">

      </xs:element>
      <xs:element name="copID" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="version" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="title" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="description" type="xs:string" minOccurs="1"  maxOccurs="1" />

    </xs:sequence>
  </xs:complexType >

  <xs:complexType name="SetIdentifier">
    <xs:sequence>
      <xs:element name="setID" type="xs:string" minOccurs="1"  maxOccurs="1" />
    </xs:sequence>
  </xs:complexType >

  <xs:complexType name="SetHeader">
    <xs:sequence>
      <xs:element name="setID" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="copID" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="title" type="xs:string" minOccurs="1"  maxOccurs="1" />
      <xs:element name="description" type="xs:string" minOccurs="1"  maxOccurs="1" />

    </xs:sequence>
  </xs:complexType >

  <xs:complexType name="Set">
    <xs:sequence>
