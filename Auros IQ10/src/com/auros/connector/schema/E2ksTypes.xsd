      <xs:element name="ruleColumnParameterName" type="xs:string"  minOccurs="1"  maxOccurs="1"/>
      <xs:element name="rowID" type="xs:int"  minOccurs="1"  maxOccurs="1"/>


      <xs:element name="instanceValue" type="xs:string"  minOccurs="1"  maxOccurs="1"/>
      <xs:element name="evaluationResult" type="xs:string"/>
      <!-- Is the result graded using a confomance state palette ? or just a numeric value -->
      <xs:element name="isGraded" type="xs:string"/>
      <xs:element name="endUserMessage" type="Message"/>
      <xs:element name="endUserFailMessage" type="Message"/>

      <!-- either supplied by client or auto grouped using the instance suffix or other schemes -->
      <xs:element name="instanceGroup" type="xs:string"/>

      <xs:element name="neededParameters" type="Parameter" minOccurs="0" maxOccurs="unbounded"/>

      <!-- list of supplied instances used for this conformance -->
      <xs:element name="suppliedParameterInstances" type="ParameterInstance"  minOccurs="0" maxOccurs="unbounded"/>
      <!-- list of previosly reported instances if any and if used for this conformance -->
      <xs:element name="reportedParameterInstances" type="ParameterInstance"  minOccurs="0" maxOccurs="unbounded"/>

      <!-- self contained list -->
      <xs:element name="computedParameterInstances" type="ParameterInstance" minOccurs="0" maxOccurs="unbounded"/>
      <!-- there will be one output parameter instance for each Conformance State,  though many are possible in a K-PAC -->
      <xs:element name="outputParameterInstanceIndex" type="xs:int"  minOccurs="0"  maxOccurs="1"/>
	  
       <!-- Runtime evaluated bounds for Max, Min, Nominal and Range column types. 
	   These bounds specify values at which conformance - pass, fail status changes.
	   In case of Range there will be 2 values in the list lower and upper -->
     <xs:element name="conformanceBounds" type="xs:string"  minOccurs="0"  maxOccurs="unbounded"/>
	  

      <xs:element name="issueIdsFromNonConformance" type="xs:string"  minOccurs="0"  maxOccurs="unbounded"/>

       <xs:element name="changeHistory" type="ChangeHistory" minOccurs="0" maxOccurs="unbounded"/>
     
    </xs:sequence>
